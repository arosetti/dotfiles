{
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "sync.autoDownload": true,
    "sync.autoUpload": true,
    "sync.forceDownload": true,
    "sync.forceUpload": true,
    "sync.quietSync": true,
    //"sync.gist": "fbea71818e9605fefa7f093860aaf43d",
    "workbench.editor.enablePreview": false,
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.settings.editor": "json",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "One Monokai",
    "workbench.startupEditor": "none",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.rename.enablePreview": false,
    "editor.fontSize": 17,
    "editor.fontLigatures": true,
    "editor.lineHeight": 22,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": false,
    "editor.tabSize": 2,
    "editor.scrollBeyondLastLine": true,
    "editor.cursorBlinking": "phase",
    "editor.linkedEditing": true,
    "editor.suggestSelection": "first",
    "editor.unicodeHighlight.invisibleCharacters": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": false,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/node_modules/*/**": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true
    },
    "explorer.autoReveal": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "terminal.explorerKind": "external",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.enableShellIntegration": true,
    "vsicons.dontShowNewVersionMessage": true,
    "cSpell.userWords": [
        "grpc",
        "Debugf",
        "Infof",
        "Tracef",
        "Warnf"
    ],
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "C_Cpp.intelliSenseEngine": "Disabled",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c11",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.addNodeAddonIncludePaths": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.clang_format_sortIncludes": true,
    "clangd.onConfigChanged": "restart",
    "clangd.path": "/home/roarc/.config/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/13.0.0/clangd_13.0.0/bin/clangd",
    "clangd.restartAfterCrash": true,
    "clangd.checkUpdates": true,
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "cmake.configureOnOpen": false,
    "go.goroot": "/snap/go/current",
    "go.inferGopath": false,
    "go.useLanguageServer": true,
    "go.gocodeAutoBuild": true,
    "go.formatTool": "gofmt",
    "go.lintTool": "golint",
    "go.lintOnSave": "workspace",
    "go.vetOnSave": "workspace",
    "go.autocompleteUnimportedPackages": true,
    "go.toolsManagement.autoUpdate": true,
    "go.coverOnTestPackage": false,
    "[go.mod]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 64,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": false
    },
    "go.languageServerExperimentalFeatures": {
        "diagnostics": true
    },
    "go.testFlags": [
        "-v"
    ],
    "gopls": {
        "usePlaceholders": true,
        "staticcheck": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "html.format.enable": true,
    "html.format.indentHandlebars": true,
    "html.format.endWithNewline": false,
    "css.lint.important": "warning",
    "[css]": {
        "editor.suggest.insertMode": "replace"
    },
    "stylelint.enable": true,
    "scss.lint.important": "warning",
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.validate": true,
    "javascript.format.enable": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "javascript.format.insertSpaceAfterConstructor": true,
    "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.format.semicolons": "remove",
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.preferences.includePackageJsonAutoImports": "on",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "[typescript]": {
        "editor.autoIndent": "full",
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescriptreact]": {
        "editor.autoIndent": "full",
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.addMissingImports": true
    },
    "eslint.format.enable": true,
    "eslint.alwaysShowStatus": true,
    "eslint.useESLintClass": true,
    "eslint.lintTask.enable": true,
    "eslint.validate": [
        "typescript"
    ],
    "emmet.includeLanguages": {
        "postcss": "css",
        "javascript": "javascriptreact",
        "markdown": "javascriptreact"
    },
    "emmet.syntaxProfiles": {
        "postcss": "css"
    },
    "emmet.triggerExpansionOnTab": true,
    "java.checkstyle.properties": {
        "basedir": "${workspaceFolder}"
    },
    "markdown.preview.scrollEditorWithPreview": true,
    "markdown.preview.scrollPreviewWithEditor": true,
    "python.languageServer": "Default",
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path.linux": "/home/roarc/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.kubectl-path.linux": "/home/roarc/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path.linux": "/home/roarc/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "redhat.telemetry.enabled": false
}
