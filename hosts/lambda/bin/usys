#!/usr/bin/env sh
emerge --sync
emerge -uaDNv --exclude chromium --autounmask --autounmask-write --with-bdeps=y --backtrack=10 --keep-going world
etc-update
pwck
grpck
emaint all -f
update-ca-certificates
eclean-dist -d
emerge --depclean -av
perl-cleaner --all
#emerge -av @preserved-rebuild
flatpak update
flatpak uninstall --unused
